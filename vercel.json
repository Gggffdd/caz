{
  "version": 2,
  "builds": [
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/public/index.html",
      "headers": {
        "Cache-Control": "no-cache",
        "Content-Security-Policy": "frame-ancestors 'self' telegram.org"
      }
    }
  ]
}
